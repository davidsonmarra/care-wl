{
  "name": "care-wl",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:DraIgraine": "cp ./env/DraIgraine.json ./env/index.json && react-native run-android --mode 'draIgraineDebug' --appIdSuffix 'draigraine'",
    "android:DrPedro": "cp ./env/DrPedro.json ./env/index.json && react-native run-android --mode 'drPedroDebug' --appIdSuffix 'drpedro'",
    "commit": "git-cz",
    "ios": "react-native run-ios",
    "ios:DraIgraine": "cp ./env/DraIgraine.json ./env/index.json && react-native run-ios --scheme 'DraIgraine'",
    "ios:DrPedro": "cp ./env/DrPedro.json ./env/index.json && react-native run-ios --scheme 'DrPedro'",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "react": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-mmkv": "^2.10.1",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3",
    "styled-components": "^6.0.7",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "miragejs": "^0.1.47",
    "prettier": "^3.0.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4",
    "xmlhttprequest": "^1.8.0"
  },
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
